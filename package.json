{
  "name": "vue3",
  "version": "0.0.4",
  "homepage": "https://jagfx.github.io/vue3-template",
  "type": "module",
  "scripts": {
    "dev": "npm run font:build && vite",
    "build": "npm run font:build && vite build",
    "build:preview": "npm run font:build && vite preview --port 8001",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "npx eslint 'src/**/*.{js,vue}' --fix",
    "lint:prettier": "npx prettier --config .prettierrc.json 'src/**/*.{js,vue}'  --write",
    "lint:stylelint": "npx stylelint src/**/*.scss",
    "test": "npm run test:unit",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "test:coverage:preview": "npm run test:coverage && npx http-server coverage/lcov-report/ -p 8002",
    "font:build": "npx fantasticon",
    "predeploy": "npm run build",
    "deploy": "npx gh-pages -d dist"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/vue3-jest": "^28.0.1",
    "bootstrap": "^5.1.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "fantasticon": "^1.2.3",
    "gh-pages": "^3.2.3",
    "http-server": "^14.1.1",
    "jest": "^28.1.3",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "vite": "^3.0.0"
  }
}
